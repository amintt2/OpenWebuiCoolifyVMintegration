# OpenWebUI Backend Environment Variables
VM_PORT=8080
WORKSPACE_DIR=/workspace

# Repository Configuration
REPO_URL=https://github.com/amintt2/OpenWebui.git
REPO_BRANCH=main

# API Configuration
API_HOST=0.0.0.0
API_TIMEOUT=120

# VM Controller Configuration
HOST_PORT=8081
COMMAND_TIMEOUT=3600
OPENWEBUI_SESSION_ID=default

# Coolify Integration
# Fill these with your actual Coolify credentials
COOLIFY_URL=https://your-coolify-instance.com
COOLIFY_API_KEY=your-api-key-here
COOLIFY_PROJECT_ID=your-project-id-here
COOLIFY_HEALTHCHECK_PATH=/api/v1/health
COOLIFY_HEALTHCHECK_PORT=8080

# Security Settings
ENABLE_AUTH=false
# JWT_SECRET=generate-a-secure-random-string

# Git repository settings (if you need to use private repositories)
# GIT_USERNAME=your-username
# GIT_TOKEN=your-personal-access-token

# Resource limits
MEMORY_LIMIT=2048m
CPU_LIMIT=1.0

# Logging Configuration
LOG_LEVEL=info
# LOG_FILE=/app/logs/backend.log

# Docker Configuration
DOCKER_MEMORY_LIMIT=2048m
DOCKER_CPU_LIMIT=1.0

# Optional: Custom configuration
VM_WORKSPACE_PATH=/path/to/custom/workspace 